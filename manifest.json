{
  "manifest_version": 2,
  "name": "kasey's extention",
  "description": "this is my attempt at a browser extention",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "*://*/*",
        "file://*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "images/icon.png"
  }
}
